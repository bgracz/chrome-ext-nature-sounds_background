{
    "manifest_version": 2,
    "name": "cozzy_noises",
    "version": "1.0.0",
    "description": "Make background noises which helps you to focus during work",
    "short_name": "cozy noises",
    "permissions": ["storage"],
    "background": {   
    "scripts": ["background.js"]
        },
    "browser_action": {
      "default_title": "cozy noises",
      "default_popup": "index.html",
      "default_icon": {
        "16": "icons/16.png",
        "32": "icons/32.png"
      }
    }
   }